import java.io.*;
import java.util.*;
class factor 
{
public static void main(String[] args) 
{
  int n, a = 0;
  Scanner x = new Scanner(System.in);
  System.out.println("Enter n value:");
  n = x.nextInt();
    for (int i = 1; i <= n; i++)
    {
      if (n % i == 0) 
      {
        a = a + 1;
      }
    }
 System.out.println("The factors are "+ a);
}
}
