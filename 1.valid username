import java.io.*;
import java.util.Scanner;
class username
{
public static void main(String[] args) 
{
        String s1,s2;
        Scanner s= new Scanner(System.in);
        System.out.println("Enter username:");
        s1=s.next();
        s2=s.next();
        if(s1==s2) 
         System.out.print("user name valid"); 
        else 
         System.out.print("user name Invalid");
}
}
